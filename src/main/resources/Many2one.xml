<?xml version='1.0' encoding='UTF-8' ?> 

class A {
int a;
}

class B {
String b;
}

class C {
int c;
String d;
}

<mappings>
    <mapping classTo="C" rel="many2one">
        <part classFrom="A" >
            <get from="a" to="c" />
        </part>
        <part classFrom="B" >
            <get from="b" to="d" />
        </part>
    </mapping>
</mappings>
